VAULTWARDEN_PORT=5009
VAULTWARDEN_WS_PORT=5010
VAULTWARDEN_BASE_PATH=/home/vaultwarden

# Public URL (used by links and callbacks)
VAULTWARDEN_DOMAIN=https://vault.example.fr

# Administration and behavior
VAULTWARDEN_ADMIN_TOKEN=changeme-admin-token
VAULTWARDEN_WEBSOCKET_ENABLED=true
VAULTWARDEN_SIGNUPS_ALLOWED=false
VAULTWARDEN_LOG_LEVEL=info
VAULTWARDEN_TZ=Europe/Paris

# Watchtower label control
VAULTWARDEN_WATCHTOWER_LABEL_ENABLE=true
